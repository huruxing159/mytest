<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.goodhope.goldselling.domain">

	<class name="Currency" table="currencies">

		<id name="id">
			<generator class="sequence">
				<param name="sequence">goldselling_id_sequence</param>
			</generator>
		</id>
		<property name="label"></property>
		<property name="symbol"></property>
		<property name="scaling_factor"></property>

	</class>


</hibernate-mapping>