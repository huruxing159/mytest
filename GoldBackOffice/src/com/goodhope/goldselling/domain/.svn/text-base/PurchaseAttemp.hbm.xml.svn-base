<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.goodhope.goldselling.domain">

	<class name="PurchaseAttemp" table="purchase_attemps">
		<id name="id">
			<generator class="sequence">
				<param name="sequence">goldselling_id_sequence</param>
			</generator>
		</id>
		<property name="gh_transaction_id"></property>
		<property name="region"></property>
		<property name="faction"></property>
		<property name="server"></property>
		<property name="character"></property>
		<property name="trade_method"></property>
		<property name="gold_amount"></property>
		<property name="price"></property>
		<property name="unit_price"></property>
		<property name="auction_item_name"></property>
		<property name="concurrency"></property>
		<property name="payment_method"></property>
		<property name="payment_method_id"></property>
		<property name="create_time"></property>

	</class>

</hibernate-mapping>