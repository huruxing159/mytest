<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.goodhope.goldselling.domain">

	<class name="TimerWatch" table="timerwatch">
		<id name="id">
			<generator class="sequence">
				<param name="sequence">goldselling_id_sequence</param>
			</generator>
		</id>
		<property name="timerName"></property>
		<property name="activeTime"></property>
	</class>

</hibernate-mapping>